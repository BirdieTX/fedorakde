alias ff='fastfetch'
alias fft='fastfetch -c $HOME/.config/fastfetch/term.jsonc'
alias neofetch='fastfetch -c neofetch'
alias nf='fastfetch -c neofetch'
alias sys-rb='$HOME/.shrc/scripts/sys-rb.sh'
alias sys-up='$HOME/.shrc/scripts/sys-up.sh'
alias update-grub='sudo grub2-mkconfig -o /etc/grub2.cfg'
